<?xml version='1.0' encoding='utf-8'?>
<widget id="com.merlinai.columbarius" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Columbarius</name>
    <description>
        Offline Merlin AI Interpreter
    </description>
    <author email="JessS@lavabit.com" href="transscendsurvival.org">
        Jess Sullivan, Grant Van Horn, Visipedia et al.
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
